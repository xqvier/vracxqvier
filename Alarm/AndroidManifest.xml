<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.xqvier.muscu" android:versionName="1.0"
	android:installLocation="auto" android:versionCode="1">
	<uses-sdk android:minSdkVersion="8" />
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true" android:theme="@android:style/Theme.NoTitleBar">
		<activity android:name="com.xqvier.muscu.Musculation"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.xqvier.muscu.alarm.AlarmConfiguration"
			android:label="@string/alarm_name" />
		<activity android:name="com.xqvier.muscu.alarm.CountDownUI"
			android:label="@string/timer_name" />
		<activity android:name="com.xqvier.muscu.alarm.ExerciseStats"
			android:label="@string/stats" />
		<activity android:name="com.xqvier.muscu.weight.WeightManager"
			android:label="@string/weight_name" />
		<activity android:name="com.xqvier.muscu.weight.WeightStats"
			android:label="@string/stats" />
		<service android:name="com.xqvier.muscu.alarm.CountDownService" />
	</application>
</manifest>